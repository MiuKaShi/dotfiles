[alias]
    s = status
    l = log --oneline
    b = checkout -b
    up = pull --rebase --autostash
    tree = log --oneline --graph --decorate --all
    upstr = rebase --rebase-merges --autostash

[merge]
    ff = no

[pull]
    ff = yes
    rebase = no

[init]
    defaultBranch = master

[credential]
    helper = cache
