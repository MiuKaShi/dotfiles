# Esteatics
ui_font     MonoLisa
font_size   38
status_bar_font_size 18
should_warn_about_user_key_override 0
dark_mode_contrast			0.6

shared_database_path    /home/miuka/.config/sioyek/shared.db

## Touchpad sensitivity
touchpad_sensitivity 0.3

# How many inches we move vertically/horizontally when performing move_* commands
horizontal_move_amount    5

# The color with which the screen is cleared before rendering the pdf
background_color    1.0 1.0 1.0
dark_mode_background_color    0 0 0

# gruvbox color.
custom_background_color  0.15 0.13 0.13
custom_text_color   0.87 0.82 0.75

# Synctex
inverse_search_command  nvr --remote-silent %1 -c %2

hover_overview	0
check_for_updates_on_startup 0
should_launch_new_window 1 
show_doc_path 1
search_url_d	https://duckduckgo.com/?q=

execute_command_a cp %1 /home/miuka/SynData 
execute_command_b bookmark %1
execute_command_e st -e cite-ext %1
execute_command_c cite-copy %1 # copy pdf bib
execute_command_l cite-link %1
execute_command_v cite-view %1
execute_command_x pdftools %1
execute_command_w webpdf %1
execute_command_o zathura %1
execute_command_z zotadd %1

